<div class="container" style="padding-top:20px;">
  <div class="row">
    <div class="span8">
      <div class="page-header">
        <h1><%= @song.title %></h1>
      </div>
      <div class="">
        <form action="/songs/<%= @song.id %>/edit" method="POST">
        <label for="Title">Title</label>
        <input type="text" name="song[title]" value=<%= @song.title.inspect %>>
        <label for="artist">Artist</label>
        <input type="text" name="song[artist]" value=<%= @song.artist.inspect %>>
        <label for="album">Album</label>
        <input type="text" name="song[album]" value=<%= @song.album.inspect %>>
        <label for="genre">Genre</label>
        <input type="text" name="song[genre]" value=<%= @song.genre.inspect %>>  
        <label for="length">Length</label>
        <input type="text" name="song[length]" value=<%= @song.length.inspect %>>
        <br>  
        <input type="submit" value='Save Changes' class="btn btn-default">            
        </form>
      </div>
    </div>
  </div>
</div>